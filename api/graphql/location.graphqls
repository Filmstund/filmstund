extend type Query {
    previousLocations: [LocationDTO!]!
    location(id: String!): LocationDTO
    filmstadenCities: [FilmstadenCityAliasDTO!]!
}

type LocationDTO {
    name: String!
    latitude: String
    longitude: String
    city: String
    cityAlias: String
    streetAddress: String
    postalCode: String
    postalAddress: String
    alias: [String!]!
}

type FilmstadenCityAliasDTO {
    name: String!
    alias: String!
}