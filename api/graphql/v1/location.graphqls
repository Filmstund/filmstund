extend type Query {
    previousLocations: [Location!]!
    location(id: String!): Location
    filmstadenCities: [FilmstadenCityAlias!]!
}

type Location {
    name: String!
    latitude: String
    longitude: String
    city: String
    cityAlias: String
    streetAddress: String
    postalCode: String
    postalAddress: String
    alias: [String!]!
}

type FilmstadenCityAlias {
    name: String!
    alias: String!
}