type Movie {
    id: UUID!
    imdbId: IMDbID
    filmstadenId: String
    slug: String
    tmdbId: TMDbID
    title: String!
    synopsis: String
    originalTitle: String
    releaseDate: String!
    productionYear: Int
    runtime: String!
    poster: String
    genres: [String!]!
    lastModifiedDate: String!
    popularity: Float!
    popularityLastUpdated: String!
    archived: Boolean!
    filmstadenShowings(city: String = "GB", afterDate: LocalDate): [FilmstadenShowing!]!
}

