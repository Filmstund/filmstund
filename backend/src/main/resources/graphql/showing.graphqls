type Showing {
    id: UUID!
    date: String
    time: String
    movie: Movie!
    location: Location
    private: Boolean!
    price: SEK
    ticketsBought: Boolean!
    admin: User!
    payToUser: User!
    expectedBuyDate: String
    participants: [Participant!]
    lastModifiedDate: String!
}

type Participant {
    user: User
    paymentType: String!
}
