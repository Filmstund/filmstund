type CurrentUser {
    id: UserID!
    name: String
    firstName: String
    lastName: String
    nick: String
    email: String!
    sfMembershipId: String
    phone: String
    avatar: String
    foretagsbiljetter: [Foretagsbiljett!]
    calendarFeedId: UUID
    calendarFeedUrl: String
    lastLogin: String!
    signupDate: String!
}

type User {
    id: UserID!
    name: String
    firstName: String
    lastName: String
    nick: String
    phone: String
    avatar: String
}

type Foretagsbiljett {
    number: String!
    expires: LocalDate!
    status: ForetagsbiljettStatus!
}

enum ForetagsbiljettStatus {
    Available
    Pending
    Used
    Expired
}

input NewUserInfo {
    nick: String
    sfMembershipId: String
    phone: String
}

input ForetagsbiljettInput {
    number: String!
    expires: LocalDate
}
